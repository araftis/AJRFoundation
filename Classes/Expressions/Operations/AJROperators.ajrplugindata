<?xml version="1.0" encoding="utf-8" ?>
<plugindata owner="com.ajr.framework.AJRFoundation" version="1">
    <extension-point name="ajroperator" class="AJRFoundation.AJROperator" registrySelector="registerOperator:properties:">
        <attribute name="class" type="class" required="YES" />
        <attribute name="precedence" type="string" required="YES" defaultValue="low" />
        <attribute name="canActAsUnary" type="boolean" required="YES" defaultValue="false" />
        <element name="operator" key="operators" type="array" required="YES">
            <attribute name="name" type="string" required="YES" />
        </element>
    </extension-point>
    
    <ajroperator class="AJRFoundation.AJRAddOperator" canActAsUnary="true" precedence="additive">
        <operator name="+" />
    </ajroperator>
    <ajroperator class="AJRFoundation.AJRAndOperator" precedence="logicalAnd">
        <operator name="&amp;&amp;" />
        <operator name="and" />
        <operator name="∧" />
    </ajroperator>
    <ajroperator class="AJRFoundation.AJRDivideOperator" precedence="multiplicative">
        <operator name="/" />
        <operator name="÷" />
    </ajroperator>
    <ajroperator class="AJRFoundation.AJREqualOperator" precedence="equality">
        <operator name="==" />
        <operator name="=" />
    </ajroperator>
    <ajroperator class="AJRFoundation.AJRExclusiveOrOperator" precedence="logicalOr">
        <operator name="xor" />
        <operator name="^^" />
        <operator name="⊻" />
    </ajroperator>
    <ajroperator class="AJRFoundation.AJRExponentOperator" precedence="multiplicative">
        <operator name="^" />
    </ajroperator>
    <ajroperator class="AJRFoundation.AJRGreaterThanOperator" precedence="relational">
        <operator name="&gt;" />
    </ajroperator>
    <ajroperator class="AJRFoundation.AJRGreaterThanOrEqualToOperator" precedence="relational">
        <operator name="&gt;=" />
        <operator name="≥" />
    </ajroperator>
    <ajroperator class="AJRFoundation.AJRIntersectOperator" precedence="bitAnd">
        <operator name="∩" />
        <operator name="intersect" />
    </ajroperator>
    <ajroperator class="AJRFoundation.AJRLessThanOperator" precedence="relational">
        <operator name="&lt;" />
    </ajroperator>
    <ajroperator class="AJRFoundation.AJRLessThanOrEqualToOperator" precedence="relational">
        <operator name="&lt;=" />
        <operator name="≤" />
    </ajroperator>
    <ajroperator class="AJRFoundation.AJRModulusOperator" precedence="multiplicative">
        <operator name="mod" />
    </ajroperator>
    <ajroperator class="AJRFoundation.AJRMultiplyOperator" precedence="multiplicative">
        <operator name="*" />
        <operator name="×" />
        <operator name="·" />
    </ajroperator>
    <ajroperator class="AJRFoundation.AJRNotEqualToOperator" precedence="equality">
        <operator name="!=" />
        <operator name="&lt;&gt;" />
        <operator name="≠" />
    </ajroperator>
    <ajroperator class="AJRFoundation.AJRNotOperator" precedence="high">
        <operator name="!" />
        <operator name="¬" />
    </ajroperator>
    <ajroperator class="AJRFoundation.AJROrOperator" precedence="logicalOr">
        <operator name="||" />
        <operator name="or" />
        <operator name="∨" />
    </ajroperator>
    <ajroperator class="AJRFoundation.AJRSubtractOperator" canActAsUnary="true" precedence="additive">
        <operator name="-" />
    </ajroperator>
    <ajroperator class="AJRFoundation.AJRUnionOperator" precedence="bitOr">
        <operator name="∪" />
        <operator name="union" />
    </ajroperator>
